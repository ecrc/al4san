prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${exec_prefix}/include

Name: ALTANAL
Description: Abstraction Layer for Task-bAsed NumericAl Libraries.
Version: @ALTANAL_VERSION_MAJOR@.@ALTANAL_VERSION_MINOR@.@ALTANAL_VERSION_MICRO@
Cflags: @ALTANAL_PKGCONFIG_DEFINITIONS@ -I${includedir}
Libs: -L${libdir} @ALTANAL_PKGCONFIG_LIBS@
Libs.private: @ALTANAL_PKGCONFIG_LIBS_PRIVATE@
Requires: @ALTANAL_PKGCONFIG_REQUIRED@
Requires.private: @ALTANAL_PKGCONFIG_REQUIRED_PRIVATE@
